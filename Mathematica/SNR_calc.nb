Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"average", " ", "over", " ", "phase"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{"hav\[Phi]", "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["h0", "2"], 
      RowBox[{"2", "\[Pi]"}]], 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox["1", 
           RowBox[{"1", "+", 
            RowBox[{"t", "/", "tgw"}]}]], ")"}], "2"], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"Cos", "[", 
             RowBox[{
              RowBox[{"\[Omega]", " ", "t"}], "+", "\[Phi]"}], "]"}], 
            RowBox[{"(", 
             FractionBox[
              RowBox[{"1", "+", 
               SuperscriptBox[
                RowBox[{"Cos", "[", "\[Iota]", "]"}], "2"]}], "2"], ")"}], 
            "Fp"}], "+", 
           RowBox[{
            RowBox[{"Sin", "[", 
             RowBox[{
              RowBox[{"\[Omega]", " ", "t"}], "+", "\[Phi]"}], "]"}], 
            RowBox[{"Cos", "[", "\[Iota]", "]"}], "Fx"}]}], ")"}], "2"]}], 
       ",", 
       RowBox[{"{", 
        RowBox[{"\[Phi]", ",", "0", ",", 
         RowBox[{"2", "\[Pi]"}]}], "}"}]}], "]"}]}], "//", "FullSimplify", 
    " "}]}]}]], "Input",
